{
    "name": "chelout/laravel-http-logger",
    "description": "A Laravel package to log HTTP requests, headers and sessions",
    "keywords": [
        "laravel",
        "http",
        "request",
        "header",
        "session",
        "logger"
    ],
    "homepage": "https://github.com/chelout/laravel-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Ostrovskiy",
            "email": "chelout@gmail.com",
            "homepage": "https://github.com/chelout/laravel-http-logger",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.6.0",
        "monolog/monolog": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chelout\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    }
}
